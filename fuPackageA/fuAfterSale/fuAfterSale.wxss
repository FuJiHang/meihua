page {
  height: 100%;
  font-size: 26rpx;
  position: relative;
  background: #ffffff;
  overflow: hidden;
}
#fuAfterSale {
  position: relative;
  color: #666;
  height: 100%;
  overflow: hidden;
}
#fuAfterSale .navTapS {
  display: flex;
  align-items: center;
  font-size: 34rpx;
  padding: 20rpx;
  box-sizing: border-box;
  font-weight: 800;
}
#fuAfterSale .navTapS image {
  height: 30rpx;
  width: 30rpx;
  margin-right: 20rpx;
}
#fuAfterSale .val {
  color: #999;
  font-size: 28rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
}
