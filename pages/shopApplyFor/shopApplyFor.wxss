/* pages/shopApplyFor/shopApplyFor.wxss */
#shopApplyFor{
  width: 100%;
  background: #FFFFFF;
  position: relative;
}
.SAP-top-box{
  width: 100%;
  display: flex;
  height: 240rpx;
  justify-content: center;
  align-items: center;
}
.SAP-top-img-box{
  width: 180rpx;
  text-align: center;
  position: relative;
  /* height: 120rpx; */
}
.SAP-top-img{
  width: 120rpx;
  position: absolute;
  left: 50%;
  margin-left: -60rpx;
  height: 120rpx;
}
.SAP-top-img-title{
  color: #666666;
  font-size: 28rpx;
  margin-top: 125rpx;
  width: 100%;
}
.SAP-center-from{
  width: 100%;
}
.SAP-center-from-list-box{
  width: 100%;
  height: 88rpx;
  display: flex;
  margin-left: 30rpx;
  /* background: red; */
  border-bottom: 1px solid #E5E5E5;
}
.SAP-center-from-right{
  width: 80rpx;
  height: 88rpx;
  /* background: red; */
  display: flex;
  /* justify-content: center; */
  align-items: center;
}
.SAP-center-from-right image{
  width: 14rpx;
  height: 26rpx;
}
.SAP-center-from-lis-title{
  height: 88rpx;
  width: 130rpx;
  text-align: center;
  line-height: 88rpx;
  color: #666666;
  font-size: 28rpx;
}
.SAP-center-from-lis-inp-box{
  flex: 1;
  height: 88rpx;
}
.SAP-center-from-lis-inp{
  color: #CCCCCC;
  width: 50%;
  height: 88rpx;
  font-size: 26rpx;
  margin-left: 100rpx;
}
.SAP-center-from-post-box{
  width: 100%;
  height: 260rpx;
}
.SAP-center-from-post-title{
  width: 100%;
  height: 88rpx;
  margin-left: 30rpx;
  display: flex;
  align-items: center;

}
.SAP-center-from-post-title span{
  font-size: 28rpx;
  color: #666666;
}
.SAP-center-from-post-title .SAP-max-img{
  color: #CCCCCC;
  margin-left: 100rpx;
}
.SAP-center-from-post-btn{
  width: 128rpx;
  height: 128rpx;
  background: red;
  margin-left: 30rpx;
}
.SAP-center-from-file{
  width: 100%;
  height: 100%;
}
.SAP-submitBtn{
  position: fixed;
  bottom: 0rpx;
  /* margin-top:  */
  height: 98rpx;
  text-align: center;
  line-height: 98rpx;
  color: #FFFFFF;
  font-size: 32rpx;
  width: 100%;
  background: #F56F68;
}