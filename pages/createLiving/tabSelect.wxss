/* pages/createLiving/tabSelect.wxss */

.TabList {
  width: 100%;
}

.close {
  width: 5vw;
  height: 5vw;
  border: 2rpx solid black;
  border-radius: 50%;
  margin-right: 20rpx;
  position: relative;
}

.close::after {
  content: "";
  position: absolute;
  left: 0;
  top: 17rpx;
  width: 100%;
  height: 5rpx;
  background-color: black;
  transform: rotate(35deg);
}

.tabs:first-child {
  border-top: 2rpx solid #e5e5e5;
}

.tabs {
  width: 100%;
  padding: 5%;
  border-bottom: 2rpx solid #e5e5e5;
  display: flex;
  justify-content: flex-start;
  align-items: center;
}

.iconNum{
  padding:0 5%;
  background-color: #F5F5F5;
  border-radius: 20rpx;
  display: flex;
  justify-content: space-between;
  align-items: center;
  margin-left: 15rpx;
  font-size: 25rpx;
  line-height: 40rpx;
  text-align: center;
  min-width: 20rpx;
  height: 40rpx;
  color: #A5A5A5;
}

.iconNum .image{
  width: 30rpx;
  height: 30rpx;
}
.iconNum .image image{
  width: 100%;
  height: 100%;
}
